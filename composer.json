{
    "name": "siciarek/symfony-common-bundle",
    "type": "library",
    "description": "Commonly used features, ordered and collected in a single bundle.",
    "keywords": [
        "symfony3",
        "utilities",
        "helpers",
        "bundle"
    ],
    "homepage": "http://github.com/siciarek/symfony-common-bundle.git",
    "license": "MIT",
    "authors": [
        {
            "name": "Jacek Siciarek",
            "email": "siciarek@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "symfony/symfony": "3.3.*",
        "doctrine/doctrine-bundle": "^1.6",
        "doctrine/doctrine-cache-bundle": "^1.2",
        "doctrine/doctrine-migrations-bundle": "1.1.1",
        "doctrine/orm": "^2.5"
    },
    "minimum-stability": "dev",
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "^2.3",
        "sensio/generator-bundle": "^3.0",
        "symfony/phpunit-bridge": "^3.3",
        "willdurand/faker-bundle": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "Siciarek\\SymfonyCommonBundle\\": ""
        }
    }
}
