{
    "name": "siciarek/symfony-common-bundle",
    "type": "library",
    "description": "Commonly used features, ordered and collected in a single bundle.",
    "keywords": [
        "symfony3",
        "utilities",
        "helpers",
        "bundle"
    ],
    "homepage": "http://github.com/siciarek/symfony-common-bundle.git",
    "license": "MIT",
    "authors": [
        {
            "name": "Jacek Siciarek",
            "email": "siciarek@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.0",
        "doctrine/doctrine-bundle": "^1.6",
        "doctrine/doctrine-cache-bundle": "^1.2",
        "doctrine/orm": "^2.5",
        "symfony/symfony": "3.4.*",
        "sensio/framework-extra-bundle": "^5.0.0",
        "knplabs/doctrine-behaviors": "^1.4@dev",
        "egulias/email-validator": "^2.1",
        "giggsey/libphonenumber-for-php": "^8.8"
    },
    "require-dev": {
        "symfony/monolog-bundle": "^3.1.0",
        "doctrine/doctrine-fixtures-bundle": "^2.3",
        "phpunit/phpunit": "^6.2",
        "symfony/phpunit-bridge": "^3.3",
        "willdurand/faker-bundle": "^1.3",
        "nelmio/alice": "^3.0@RC",
        "symfony/web-server-bundle": "^3.3"
    },
    "autoload": {
        "psr-4": {
            "Siciarek\\SymfonyCommonBundle\\": "",
            "": "app"
        }
    }
}
